package mencoba;

        import java.util.ArrayList;
        import java.util.Collection;
        import java.util.List;

public class list{

    public static void main(String[] args) {

        List<String> alat = new ArrayList<>();
        alat.add("gitar"); // method add() untuk menambahkan objek ke List
        alat.add("piano");
        alat.add("drum");
        alat.add("biola");
        alat.add("gangsa");
        alat.add("ceng-ceng");
        System.out.println("List alat musik: ");
        for (int i = 0; i < alat.size(); i++) {
            System.out.println("\t index-"+ i +" = " + alat.get(i));
        }
        alat.remove("venus"); // method remove() untuk mengeluarkan objek dari List
        System.out.println("List alat musik bali:");
        for (int i = 4; i < alat.size(); i++) {
            System.out.println("\t index-"+ i +" = " + alat.get(i));
        }
    }
}
